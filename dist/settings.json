{
  /* -------------------------------------------------------------------------- */
  /*                               FUNCTIONALITY                                */
  /* -------------------------------------------------------------------------- */

  /* ------------------------------ Sticky scroll ----------------------------- */
  "editor.stickyScroll.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "terminal.integrated.stickyScroll.enabled": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "editor.suggest.preview": true,

  /* ------------------------------- Suggestions ------------------------------ */
  "editor.snippetSuggestions": "inline",
  "editor.quickSuggestions": {
    "other": "on",
    "strings": "on",
    "comments": "on",
  },
  // "editor.quickSuggestionsDelay": 50,

  "editor.tabCompletion": "on",
  "editor.inlineSuggest.showToolbar": "always",
  "editor.inlineSuggest.enabled": true,

  /* --------------------- File exclusion for performance --------------------- */

  // https://medium.com/@krtirtho/make-visual-studio-code-less-ram-consuming-faster-90db8c76a187
  // "files.exclude": {
  //   "**/yarn.lock/**": true,
  //   "**/package-lock.json": true
  //   // "**/node_modules/**": true
  // },
  // "explorer.excludeGitIgnore": true,

  "json.schemas": [
    {
      "exclude": ["node_modules"],
    },
  ],

  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    // "**/env": true,
    // "**/venv": true
  },

  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    // "**/env/**": true,
    // "**/venv/**": true,
    // "env-*": true
  },

  /* -------------------------------- Explorer -------------------------------- */
  "explorer.compactFolders": false,
  "explorer.copyRelativePathSeparator": "/",

  "workbench.startupEditor": "none",
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.expandMode": "doubleClick",

  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",

  /* --------------------------------- Update --------------------------------- */
  "extensions.autoUpdate": false,
  "update.mode": "default",
  "update.showReleaseNotes": true,

  /* -------------------------------------------------------------------------- */
  /*                                 APPEARANCE                                 */
  /* -------------------------------------------------------------------------- */
  "vscode_custom_css.imports": ["file:///Users/hareki/.config/vscode/custom-styles.css"],

  /* -------------------------------- Title bar -------------------------------- */
  "window.titleBarStyle": "custom",
  // "window.title": "${dirty}Hareki's Secret Lab",
  // "window.titleSeparator": " | ",
  "window.customTitleBarVisibility": "windowed",

  /* -------------------------------- Menu bar -------------------------------- */
  "window.customMenuBarAltFocus": false,
  "window.menuBarVisibility": "toggle",

  /* ---------------------------------- Font ---------------------------------- */
