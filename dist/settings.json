  /* ----------------------------------- VIM Configurations ---------------------------------- */
  "editor.lineNumbers": "relative",

  "vim.easymotionDimBackground": false,
  "vim.autoindent": false,
  "vim.easymotion": true,
  // "vim.foldfix": true,
  "vim.enableNeovim": false,
  "vim.ignorecase": true,

  "vim.timeout": 500,
  "vim.history": 15,
  "vim.mouseSelectionGoesIntoVisualMode": true,
  "vim.overrideCopy": true,
  "vim.camelCaseMotion.enable": true,

  "vim.leader": "<Space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["/"], "commands": ["actions.find"], "silent": true },
    {
      "before": ["?"],
      "commands": ["editor.action.startFindReplaceAction"],
      "silent": true,
    },

    // Need to duplicate the settings here,
    // since if we only do it in the keybindings.json, the "space" will never get registered to vim
    // meaning it will  potentially break stuff (need further investigation)
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"],
      "silent": true,
    },

    {
      "before": ["<leader>", "e"],
      "commands": ["revealInExplorer", "workbench.files.action.focusFilesExplorer"],
      "silent": true,
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.view.scm"],
      "silent": true,
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["workbench.action.quickTextSearch"],
      "silent": true,
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.findInFiles"],
      "silent": true,
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["workbench.action.replaceInFiles"],
      "silent": true,
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["todo-tree-view.focus"],
      "silent": true,
    },

    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.panel.chat.view.edits.focus"],
      "silent": true,
    },
    {
      "before": ["<leader>", "x"],
      "commands": ["workbench.view.extensions"],
      "silent": true,
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["workbench.view.debug"],
      "silent": true,
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.focusPanel"],
      "silent": true,
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.showCommands"],
      "silent": true,
    },

    // Multi-curor
    {
      "before": ["<C-m>"],
      "after": ["g", "b"],
    },
    {
      "before": ["w"],
      "after": ["<leader>", "w"],
    },
    {
      "before": ["b"],
      "after": ["<leader>", "b"],
    },
    {
      "before": ["<C-a>"],
      "after": ["v", "i", "e"],
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"],
      "silent": true,
    },
    {
      "before": ["g", "r"],
      "commands": ["references-view.findReferences"],
      "silent": true,
    },

    { "before": ["P"], "after": ["\"", "0", "p"] },
    {
      "before": ["Y"],
      "after": ["y", "$"],
    },
    {
      "before": ["C"],
      "after": ["c", "$"],
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-m>"],
      "after": ["g", "b"],
    },
    {
      "before": ["p"],
      "after": ["\"", "_", "d", "P"],
    },
    {
      "before": ["x"],
