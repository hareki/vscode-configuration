      "sideBarStickyScroll.background": "#2A2A42",
      // "sideBarStickyScrollHover.background": "#31324B",
      "sideBarStickyScroll.shadow": "#0000",

      "editor.background": "#181825",
      "editorGutter.background": "#181825",
      "minimap.background": "#181825",
      "statusBar.background": "#181825",

      "sideBar.background": "#1e1e2e",
      "terminal.background": "#1e1e2e",
      "sideBarSectionHeader.background": "#1e1e2e",

      "panel.border": "#0000",
    },
  },

  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000",
          },
        },
      ],
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff",
          },
        },
      ],
    },
    "textMateRules": [],
  },

  "editor.mouseWheelScrollSensitivity": 2.5,
  "workbench.list.mouseWheelScrollSensitivity": 2.5,

  /* --------------------------------- Minimap -------------------------------- */
  "editor.minimap.side": "left",
  "editor.minimap.scale": 2,
  "editor.minimap.showSlider": "always",
  "editor.minimap.maxColumn": 80,

  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,

  /* -------------------------------- Terminal -------------------------------- */
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.title": "${cwdFolder}",
  "terminal.integrated.tabs.description": "",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
      "args": ["-NoLogo"],
    },
  },
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.gpuAcceleration": "auto",
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.mouseWheelScrollSensitivity": 2.5,

  /* ---------------------------------- Tabs ---------------------------------- */
  "workbench.editor.untitled.labelFormat": "name",
  "workbench.editor.customLabels.patterns": {
    "**/src/**/index.{ts,tsx,js,jsx}": "[I] | ${dirname}.${extname}",
  },
  "workbench.editor.labelFormat": "default",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.closeEmptyGroups": true,
  "breadcrumbs.enabled": true,

  /* ------------------------------ Bracket pairs ----------------------------- */
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": false,

